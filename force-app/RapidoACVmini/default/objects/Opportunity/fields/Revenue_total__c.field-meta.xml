<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Revenue_total__c</fullName>
    <description>Includes Revenue Won and Lost (churn)</description>
    <formula>IF(!(IsClosed &amp;&amp; !IsWon),
 ExpectedRevenue ,
0)

+
Revenue_churn__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>_Revenue total</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
